{
  "name": "@spaceflow/scripts",
  "version": "0.25.0",
  "description": "Spaceflow 脚本插件，用于在分支锁定/解锁之间执行 JS 语句",
  "license": "MIT",
  "author": "Lydanne",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lydanne/spaceflow.git",
    "directory": "extensions/scripts"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "spaceflow build",
    "dev": "spaceflow dev"
  },
  "devDependencies": {
    "@spaceflow/cli": "workspace:*",
