{
  "name": "@spaceflow/cli",
  "version": "0.23.0",
  "description": "Spaceflow CLI 工具",
  "license": "MIT",
  "author": "Lydanne",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lydanne/spaceflow.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "bin": {
    "space": "dist/cli.js",
    "spaceflow": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "rspack build -c rspack.config.mjs",
    "format": "oxfmt src --write",
    "lint": "oxlint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@rspack/cli": "^1.7.4",
    "@rspack/core": "^1.7.4",
    "@spaceflow/core": "workspace:*",
    "commander": "^12.1.0",
    "json-stringify-pretty-compact": "^4.0.0",
    "micromatch": "^4.0.8",
    "zod-to-json-schema": "^3.25.1"
  },
  "peerDependencies": {
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "ts-node": "catalog:",
    "tsconfig-paths": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "vitest": "catalog:"
  }
}
